'..............................................................................
sub ShowAParametricMessage(S)
dim DefaultMessage

    DefaultMessage = "Hello World!"

    If S = "" Then
        ShowMessage(DefaultMessage)
    Else
        ShowMessage(S)
    End If
end sub
